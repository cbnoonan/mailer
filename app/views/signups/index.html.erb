<h1>Listing signups</h1>
 
<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Email</th>
    <th></th>
  </tr>
 
  <% @signups.each do |s| %>
    <tr>
      <td><%= s.first_name %></td>
      <td><%= s.last_name %></td>
      <td><%= s.email %></td>
      <td><%= link_to 'Delete', signup_path(s),
                          method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'New signup', new_signup_path %>